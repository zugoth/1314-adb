<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
" http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="fajki.model.Firma" table="FIRMA" lazy="true" dynamic-update="true" select-before-update="true">
	<id name="idFirmy" column="IDFIRMY" type="java.lang.Long"></id>
	
	<property name="nip" column="NIP" type="java.lang.Integer"></property>
	<property name="krs" column="KRS" type="java.lang.Integer"></property>
	<property name="nazwa" column="NAZWA" type="java.lang.String"></property>
	<property name="idAdres" column="IDADRES" type="java.util.Long"></property>

</class>
</hibernate-mapping>